<T3DataStructure>
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>Options</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <settings.subject>
            <label>Mail Subject</label>
            <config>
              <type>input</type>
            </config>
          </settings.subject>
          <settings.receiver>
              <label>Admin Mail Address</label>
              <config>
                <type>input</type>
              </config>
          </settings.receiver>
          <settings.form>
            <label>Form Type</label>
            <config>
              <type>select</type>
              <items type="array">
                <numIndex index="0" type="array">
                  <numIndex index="0">Feedback</numIndex>
                  <numIndex index="1">feedback</numIndex>
                </numIndex>
                <numIndex index="1" type="array">
                  <numIndex index="0">Medizin</numIndex>
                  <numIndex index="1">medical</numIndex>
                </numIndex>
                <numIndex index="3" type="array">
                  <numIndex index="0">Chirurgie</numIndex>
                  <numIndex index="1">surgery</numIndex>
                </numIndex>
                <numIndex index="4" type="array">
                  <numIndex index="0">Gyn√§kologie</numIndex>
                  <numIndex index="1">gynecology</numIndex>
                </numIndex>
                <numIndex index="5" type="array">
                  <numIndex index="0">Radiologie</numIndex>
                  <numIndex index="1">radiology</numIndex>
                </numIndex>
              </items>
            </config>
          </settings.form>
          <settings.default>
            <label>Load Default Forms (Do not use with Feedback)</label>
            <config>
              <type>check</type>
            </config>
          </settings.default>
          <settings.title>
            <label>Hide title</label>
            <config>
              <type>check</type>
            </config>
          </settings.title>
          <settings.birthdate>
            <label>Show Birthdate</label>
            <config>
              <type>check</type>
            </config>
          </settings.birthdate>
        </el>
      </ROOT>
    </sDEF>
  </sheets>
</T3DataStructure>
