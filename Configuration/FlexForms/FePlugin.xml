<T3DataStructure>
    <sheets>
        <form>
            <ROOT>

                <TCEforms>
                    <sheetTitle>Form</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.inputtext>
                        <section>1</section>
                        <type>array</type>
                        <el>
                            <textfield>
                                <title>Textfield</title>
                                <type>array</type>
                                <el>
                                    <name>
                                        <TCEforms>
                                            <label>Name</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </name>
                                    <label>
                                        <TCEforms>
                                            <label>Label</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </label>
                                    <placeholder>
                                        <TCEforms>
                                            <label>Placeholder</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </placeholder>
                                    <inputtype>
                                        <TCEforms>
                                            <label>Input Type</label>
                                            <config>
                                                <type>select</type>
                                                <items type="array">
                                                    <numIndex index="0" type="array">
                                                        <numIndex index="0">Text</numIndex>
                                                        <numIndex index="1">text</numIndex>
                                                    </numIndex>
                                                    <numIndex index="1" type="array">
                                                        <numIndex index="0">Email</numIndex>
                                                        <numIndex index="1">email</numIndex>
                                                    </numIndex>
                                                    <numIndex index="2" type="array">
                                                        <numIndex index="0">Tel</numIndex>
                                                        <numIndex index="1">tel</numIndex>
                                                    </numIndex>
                                                    <numIndex index="3" type="array">
                                                        <numIndex index="0">Date</numIndex>
                                                        <numIndex index="1">date</numIndex>
                                                    </numIndex>
                                                    <numIndex index="4" type="array">
                                                        <numIndex index="0">Number</numIndex>
                                                        <numIndex index="1">number</numIndex>
                                                    </numIndex>
                                                    <numIndex index="5" type="array">
                                                        <numIndex index="0">Search</numIndex>
                                                        <numIndex index="1">search</numIndex>
                                                    </numIndex>
                                                </items>
                                            </config>
                                        </TCEforms>
                                    </inputtype>
                                    <required>
                                        <TCEforms>
                                            <label>Required</label>
                                            <config>
                                                <type>check</type>
                                            </config>
                                        </TCEforms>
                                    </required>
                                </el>
                            </textfield>
                            <textarea>
                                <title>Textarea</title>
                                <type>array</type>
                                <el>
                                    <name>
                                        <TCEforms>
                                            <label>Name</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </name>
                                    <label>
                                        <TCEforms>
                                            <label>Label</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </label>
                                    <placeholder>
                                        <TCEforms>
                                            <label>Placeholder</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </placeholder>
                                    <required>
                                        <TCEforms>
                                            <label>Required</label>
                                            <config>
                                                <type>check</type>
                                            </config>
                                        </TCEforms>
                                    </required>
                                </el>
                            </textarea>
                            <checkbox>
                                <title>Checkbox</title>
                                <type>array</type>
                                <el>
                                    <name>
                                        <TCEforms>
                                            <label>Name</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </name>
                                    <label>
                                        <TCEforms>
                                            <label>Label</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </label>
                                    <required>
                                        <TCEforms>
                                            <label>Required</label>
                                            <config>
                                                <type>check</type>
                                            </config>
                                        </TCEforms>
                                    </required>
                                </el>
                            </checkbox>
                            <radio>
                                <title>Radiobutton</title>
                                <type>array</type>
                                <el>
                                    <name>
                                        <TCEforms>
                                            <label>Name</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </name>
                                    <label>
                                        <TCEforms>
                                            <label>Label</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </label>
                                    <required>
                                        <TCEforms>
                                            <label>Required</label>
                                            <config>
                                                <type>check</type>
                                            </config>
                                        </TCEforms>
                                    </required>
                                </el>
                            </radio>
                            <fieldset-start>
                                <title>Fieldset start</title>
                                <type>array</type>
                                <el>
                                    <legend>
                                        <TCEforms>
                                            <label>Legend(Fieldset Headline)</label>
                                            <config>
                                                <type>input</type>
                                                <size>48</size>
                                            </config>
                                        </TCEforms>
                                    </legend>
                                </el>
                            </fieldset-start>
                            <fieldset-end>
                                <title>Fieldset End</title>
                                <type>array</type>
                                <el>
                                    <legend>
                                        <TCEforms>
                                            <label>To Ignore</label>
                                            <config>
                                                <type>check</type>
                                            </config>
                                        </TCEforms>
                                    </legend>
                                </el>
                            </fieldset-end>
                        </el>
                    </settings.inputtext>
                </el>
            </ROOT>
        </form>
        <options>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Options</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.subject>
                        <label>Mail Subject</label>
                        <config>
                            <type>input</type>
                        </config>
                    </settings.subject>
                    <settings.receiver>
                        <label>Admin Mail Address</label>
                        <config>
                            <type>input</type>
                        </config>
                    </settings.receiver>
                    <settings.form>
                        <label>Form Type</label>
                        <config>
                            <type>select</type>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <numIndex index="0">Feedback</numIndex>
                                    <numIndex index="1">feedback</numIndex>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <numIndex index="0">Medizin</numIndex>
                                    <numIndex index="1">medical</numIndex>
                                </numIndex>
                                <numIndex index="3" type="array">
                                    <numIndex index="0">Chirurgie</numIndex>
                                    <numIndex index="1">surgery</numIndex>
                                </numIndex>
                                <numIndex index="4" type="array">
                                    <numIndex index="0">Gyn√§kologie</numIndex>
                                    <numIndex index="1">gynecology</numIndex>
                                </numIndex>
                                <numIndex index="5" type="array">
                                    <numIndex index="0">Radiologie</numIndex>
                                    <numIndex index="1">radiology</numIndex>
                                </numIndex>
                            </items>
                        </config>
                    </settings.form>
                    <settings.default>
                        <label>Load Default Forms (Do not use with Feedback)</label>
                        <config>
                            <type>check</type>
                        </config>
                    </settings.default>
                    <settings.title>
                        <label>Hide title</label>
                        <config>
                            <type>check</type>
                        </config>
                    </settings.title>
                    <settings.birthdate>
                        <label>Show Birthdate</label>
                        <config>
                            <type>check</type>
                        </config>
                    </settings.birthdate>
                </el>
            </ROOT>
        </options>
        <responsemails>
            <ROOT>
            <TCEforms>
                <sheetTitle>Antwort Mails</sheetTitle>
            </TCEforms>
            <type>array</type>
            <el>
                <settings.usermail>
                    <TCEforms type="array">
                        <label>User Mail</label>
                        <config type="array">
                            <type>text</type>
                            <cols>48</cols>
                            <rows>5</rows>
                        </config>
                        <defaultExtras>richtext[*]:rte_transform[flag=rte_enabled|mode=ts_css]</defaultExtras>
                    </TCEforms>
                </settings.usermail>
                <settings.adminmail>
                    <TCEforms type="array">
                        <label>Admin Mail</label>
                        <config type="array">
                            <type>text</type>
                            <cols>48</cols>
                            <rows>5</rows>
                        </config>
                        <defaultExtras>richtext[*]:rte_transform[flag=rte_enabled|mode=ts_css]</defaultExtras>
                    </TCEforms>
                </settings.adminmail>
            </el>
            </ROOT>
        </responsemails>
    </sheets>
</T3DataStructure>